<p id="notice"><%= notice %></p>

<h1>Listing Contact Apps</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Phone</th>
      <th>Telephone</th>
      <th>Work phone</th>
      <th>Email id</th>
      <th>Work email</th>
      <th>Github id</th>
      <th>Facebook id</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contact_apps.each do |contact_app| %>
      <tr>
        <td><%= contact_app.First_name %></td>
        <td><%= contact_app.Last_Name %></td>
        <td><%= contact_app.Phone %></td>
        <td><%= contact_app.Telephone %></td>
        <td><%= contact_app.Work_Phone %></td>
        <td><%= contact_app.Email_Id %></td>
        <td><%= contact_app.Work_Email %></td>
        <td><%= contact_app.Github_Id %></td>
        <td><%= contact_app.Facebook_Id %></td>
        <td><%= link_to 'Show', contact_app %></td>
        <td><%= link_to 'Edit', edit_contact_app_path(contact_app) %></td>
        <td><%= link_to 'Destroy', contact_app, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contact app', new_contact_app_path %>
